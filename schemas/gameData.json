{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "array",
    "maxItems": 7,
    "items": {
        "type": "object",
        "properties": {
            "winner": {
                "type": "string",
                "enum": ["none", "alpha", "bravo"]
            },
            "color": {
                "type": "object",
                "allOf": [
                    {
                        "$ref": "defs/color.json#/color"
                    },
                    {
                        "properties": {
                            "colorsSwapped": {
                                "type": "boolean"
                            }
                        },
                        "required": ["colorsSwapped"]
                    }
                ]
            }
        },
        "required": ["winner"]
    },
    "default": [
        {
            "winner": "none",
            "color": {
                "index": 0,
                "title": "Green vs Grape",
                "clrA": "#37FC00",
                "clrB": "#7D28FC",
                "categoryName": "Ranked Modes",
                "colorsSwapped": false
            }
        },
        {
            "winner": "none",
            "color": {
                "index": 1,
                "title": "Green vs Magenta",
                "clrA": "#04D976",
                "clrB": "#D600AB",
                "categoryName": "Ranked Modes",
                "colorsSwapped": false
            }
        },
        {
            "winner": "none",
            "color": {
                "index": 2,
                "title": "Turquoise vs Orange",
                "clrA": "#10E38F",
                "clrB": "#FB7B08",
                "categoryName": "Ranked Modes",
                "colorsSwapped": false
            }
        }
    ]
}
