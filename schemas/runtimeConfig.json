{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "gameVersion": {
            "type": "string",
            "enum": ["SPLATOON_2", "SPLATOON_3"]
        },
        "locale": {
            "type": "string",
            "enum": ["EN", "DE", "EU_FR", "JA"]
        },
        "interfaceLocale": {
            "type": "string",
            "enum": ["EN", "JA"]
        }
    },
    "required": ["gameVersion", "locale", "interfaceLocale"],
    "additionalProperties": false,
    "default": {
        "gameVersion": "SPLATOON_3",
        "locale": "EN",
        "interfaceLocale": "EN"
    }
}
