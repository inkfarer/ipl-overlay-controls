{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "startTime": {
            "type": "string",
            "format": "date-time"
        },
        "isVisible": {
            "type": "boolean"
        }
    },
    "required": ["startTime", "isVisible"],
    "default": {
        "startTime": "1970-01-01T00:00:00+00:00",
        "isVisible": false
    }
}
