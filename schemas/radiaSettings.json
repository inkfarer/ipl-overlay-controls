{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "guildID": {
            "type": "string"
        },
        "enabled": {
            "type": "boolean"
        },
        "updateOnImport": {
            "type": "boolean"
        },
        "connectedChannel": {
            "type": "string"
        }
    },
    "required": ["guildID", "enabled", "updateOnImport"],
    "additionalProperties": false,
    "default": {
        "guildID": "",
        "enabled": false,
        "updateOnImport": false
    }
}
