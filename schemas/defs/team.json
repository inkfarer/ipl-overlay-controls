{
    "$schema": "http://json-schema.org/draft-07/schema",
    "team": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string"
            },
            "name": {
                "type": "string"
            },
            "logoUrl": {
                "type": "string"
            },
            "players": {
                "type": "array",
                "items": {
                    "$ref": "player.json#/player"
                }
            }
        },
        "additionalProperties": true,
        "required": ["id", "name", "players"]
    }
}
