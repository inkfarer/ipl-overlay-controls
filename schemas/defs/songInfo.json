{
    "$schema": "http://json-schema.org/draft-07/schema",
    "songInfo": {
        "type": "object",
        "properties": {
            "artist": {
                "type": "string"
            },
            "song": {
                "type": "string"
            }
        },
        "required": ["song"],
        "default": {
            "artist": "Unknown Artist",
            "song": "Unknown Song"
        }
    }
}
