{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "screenshotParser": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "saveBadScreenshotsToDisk": { "type": "boolean" }
            }
        },
        "lastfm": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "apiKey": { "type": "string" },
                "secret": { "type": "string" }
            }
        },
        "smashgg": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "apiKey": { "type": "string" }
            }
        },
        "radia": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "url": { "type": "string" },
                "socketUrl": { "type": "string" },
                "authentication": { "type": "string" }
            }
        },
        "sendouInk": {
            "type": "object",
            "additionalProperties": false,
            "required": ["apiKey"],
            "properties": {
                "apiKey": { "type": "string" }
            }
        }
    }
}
